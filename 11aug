import java.util.*;
class One
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a;
        int k=1;
        a=sc.nextInt();
        for(int i=2;i<=a;i++)
        {
           k=k*i; 
        }
        System.out.println(k);
        
    }
}
